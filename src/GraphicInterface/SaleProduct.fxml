<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>

<VBox fx:id="panelSaleProductList" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GraphicInterface.SaleController">
    <children>
        <ToolBar prefHeight="25.0" prefWidth="200.0">
            <items>
                <Label text="Quantidade " />
                <TextField fx:id="txtQuantityProduct"  />
                <Button onAction="#selectProduct" text="Selecionar" />
            </items>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </ToolBar>
        <TableView fx:id="tableSaleProduct">
            <columns>
                <TableColumn fx:id="tableColumnSaleProductCode" prefWidth="75.0" text="Código" />
                <TableColumn fx:id="tableColumnSaleProductName" prefWidth="200.0" text="Nome" />
                <TableColumn fx:id="tableColumnSaleProductPrice" prefWidth="75.0" text="Preço" />
            </columns>
        </TableView>
    </children>
</VBox>
