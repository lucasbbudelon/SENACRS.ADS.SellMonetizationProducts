<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>

<VBox fx:id="panelList" prefHeight="350.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GraphicInterface.CustomerController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button mnemonicParsing="false" onAction="#create" text="Cadastrar" />
            <Button mnemonicParsing="false" onAction="#update" text="Editar" />
            <Button mnemonicParsing="false" onAction="#delete" text="Remover" />
        </items>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ToolBar>
      <TableView fx:id="tableViewCustomers" prefHeight="314.0" prefWidth="420.0">
        <columns>
          <TableColumn fx:id="tableColumnCPF" prefWidth="113.0" text="CPF" />
          <TableColumn fx:id="tableColumnName" prefWidth="111.0" text="Nome" />
            <TableColumn fx:id="tableColumnEmail" prefWidth="189.0" text="E-Mail" />
        </columns>
      </TableView>
   </children>
</VBox>
